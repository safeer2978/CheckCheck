<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.checkcheck.ui.home.HomeFragment"
        android:label="Today's Tasks"
        tools:layout="@layout/fragment_task_list" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.checkcheck.ui.task_list.TaskList"
        android:label="Calender"
        tools:layout="@layout/fragment_task_list" />
    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.checkcheck.ui.settings.SettingsFragment"
        android:label="Settings"
        tools:layout="@layout/fragment_notifications" />

</navigation>